<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    <link href="~/Content/StyleAdmin.css" rel="stylesheet" />
</head>
<body style="margin-top:0px;">
    <div id="banner" style="top:0px;position:absolute;">HỆ THỐNG QUẢN LÝ KÝ TÚC XÁ </div>
    <div id="menu" style="margin-top:10px;">
        <div>
            <div class="menu-items">
                <div class="menu-item">
                    @Html.ActionLink("Sinh viên", "Index", "sinh_vienUser", "", new { @class = "a" })
                </div>
                <div class="menu-item">
                    @Html.ActionLink("Phòng", "Index", "PhongUser", "", new { @class = "a" })
                </div>
                <div class="menu-item">
                    @Html.ActionLink("Dịch vụ", "Index", "dich_vuUser", "", new { @class = "a" })
                </div>
                <div class="col-md-10">@Html.Partial("_LoginPartial")</div>
            </div>
            <hr style="display: block;border-top: 1px solid aqua;  margin: 8px 0;" />
    </div>
    <div id="content">
        @RenderBody()
    </div>
</body>
</html>
